	</distributions>
	
	<mcmc niter="100" seed="-1255892792" screen_update="1">
		<uniform_proposal parameter="X" half-width="1.0" weight="1"/>
		
		<log burnin="0" thinning="20" file="test.mn.mcmc.txt">
			<parameter idref="X"/>
<!--			<parameter idref="Sigma"/> -->
